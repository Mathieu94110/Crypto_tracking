{
    "compilerOptions": {
        "target": "es5",
        "sourceMap": true,
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
            "typeRoots": [
                "node_modules/@types",
                "src/imagesTypes"
            ],
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": false,
        "suppressImplicitAnyIndexErrors": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "jsx": "react",
        "removeComments": true
    },
    "include": [
        "src"
    ]
}